<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="kr.co.doby.web.repository.WithPeriodRepository">

    <select id="findById" resultType="WithPeriod">
        select
            *
        from
            with_period
        where id = #{id};
    </select>

</mapper>